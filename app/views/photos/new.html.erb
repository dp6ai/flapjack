<% @photo.errors.full_messages.each do |message| %>
    <p><%= message %></p>
<% end %>

<p>New Photo</p>
<%= form_for @photo, :url => photos_path, :html => { :multipart => true } do |f| %>
  
  <%= f.label :caption %>
  <%= f.text_field :caption %>

  <%= f.label :photo %>
  <%= f.file_field :photo %>

  <%= f.submit %>
<% end %>